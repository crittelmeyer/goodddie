{
  "name": "@crittelmeyer/components",
  "version": "12.19.0",
  "license": "UNLICENSED",
  "repository": "https://github.com/crittelmeyer/phoenix-frontend-components.git",
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "analyze": "ANALYZE=true yarn build && open stats.html",
    "build": "rollup -c",
    "component": "hygen new component",
    "dev:build": "nodemon -e js,jsx,ts,tsx --ignore dist/ -x yarn build",
    "dev:push": "nodemon --watch dist -x yalc push --private",
    "dev": "yarn dev:build & yarn dev:push",
    "lint": "eslint src/components src/constant src/styles src/types src/utils --ext=ts,tsx",
    "lint:fix": "eslint src/components src/constant src/styles src/types src/utils --ext=ts,tsx --fix",
    "prepare": "husky install",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "storybook": "start-storybook -p 6006 -s public",
    "storybook:build": "build-storybook -s public"
  },
  "main": "dist/components.cjs.js",
  "module": "dist/components.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@contentful/rich-text-react-renderer": "^15.11.1",
    "@babel/core": "^7.17.0",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.2",
    "@mui/lab": "^5.0.0-alpha.81",
    "@mui/material": "^5.7.0",
    "@mui/styles": "^5.7.0",
    "@crittelmeyer/eslint-config": "^9.0.2",
    "@crittelmeyer/eslint-plugin": "^2.0.1",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@rollup/plugin-url": "^6.1.0",
    "@storybook/addon-actions": "^6.5.5",
    "@storybook/addon-essentials": "^6.5.5",
    "@storybook/addon-interactions": "^6.5.5",
    "@storybook/addon-links": "^6.5.5",
    "@storybook/builder-webpack5": "^6.5.5",
    "@storybook/core": "^6.5.5",
    "@storybook/manager-webpack5": "^6.5.5",
    "@storybook/react": "^6.5.5",
    "@storybook/testing-library": "^0.0.11",
    "@types/node": "^17.0.33",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@types/webpack-env": "^1.17.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "axios": "^0.27.2",
    "babel-loader": "^8.2.5",
    "contentful": "^9.1.28",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "eslint-plugin-storybook": "^0.5.6",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.0",
    "hygen": "^6.1.0",
    "is-plain-object": "^5.0.0",
    "lint-staged": "^11.0.0",
    "next": "12.1.7-canary.5",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.4",
    "prettier": "^2.0.5",
    "prop-types": "^15.5.6",
    "react": "^18.1.0",
    "react-device-detect": "^2.1.2",
    "react-dom": "^18.1.0",
    "react-ga": "^3.3.0",
    "require-from-string": "^2.0.2",
    "rollup": "^2.63.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "standard-version": "^9.3.2",
    "tslib": "^2.3.1",
    "tss-react": "^3.6.2",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1"
  },
  "peerDependencies": {
    "@emotion/react": ">=11.4.1",
    "@emotion/styled": ">=11.3.0",
    "@mui/icons-material": ">=5.0",
    "@mui/lab": ">=5.0.0-alpha.49",
    "@mui/material": ">=5.0.2",
    "@mui/styles": ">=5.0.1",
    "axios": ">=0.24.0",
    "next": ">=12.1.0",
    "react": ">=17.0",
    "react-device-detect": ">=2.1.2",
    "react-dom": ">=17.0.2",
    "react-ga": ">=3.3.0",
    "tss-react": ">=3.6.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint"
    ]
  },
  "resolutions": {
    "@emotion/utils": "1.1.0"
  },
  "dependencies": {}
}
